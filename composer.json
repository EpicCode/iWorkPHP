{
    "name": "iworkphp/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"iWorkPHP Standard Edition\" distribution",
	"autoload": {
		"psr-4": {
			"iWorkPHP\\" : "framework/library/",
			"Controller\\" : "app/controllers/",
			"Model\\" : "app/models/",
			"Db\\Model\\" : "app/databases/default/models/",
			"Db\\Repo\\" : "app/databases/default/repos/"
		}
	},
    "require": {
        "twig/twig": "1.*",
		"ezyang/htmlpurifier": "~4",
		"symfony/yaml": "2.4.*",
		"symfony/http-foundation": "2.4.*",
		 "doctrine/orm": "*"
    }
}